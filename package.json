{
  "name": "feedsme",
  "version": "3.0.0",
  "description": "A changes feed micro service",
  "main": "./lib/index.js",
  "bin": {
    "feedsme": "bin/server"
  },
  "scripts": {
    "test": "nyc --reporter=text --reporter=lcov npm run test:mocha",
    "test:mocha": "mocha test/*.test.js",
    "watch": "npm run test:mocha -- --watch",
    "start": "bin/server",
    "lint": "eslint -c .eslintrc --fix './{lib,test}/**/*.js'",
    "build": "mmdc -i diagram/code/build.mmd -o diagram/svg/build.svg",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/godaddy/feedsme.git"
  },
  "keywords": [
    "changes",
    "_changes",
    "feed",
    "feedme",
    "feedsme"
  ],
  "author": "GoDaddy Operating Company, LLC",
  "contributors": [
    "Arnout Kazemier",
    "Martijn Swaagman",
    "Jarrett Cruger",
    "Charlie Robbins"
  ],
  "license": "MIT",
  "dependencies": {
    "@wrhs/release-line": "^1.1.1",
    "async": "^1.4.0",
    "body-parser": "^1.19.0",
    "carpenterd-api-client": "^2.0.0",
    "datastar": "^1.7.0",
    "errs": "^0.3.0",
    "eventemitter3": "^1.1.0",
    "express-async-handler": "^1.1.4",
    "json-stream": "^1.0.0",
    "make-promises-safe": "^1.1.0",
    "one-time": "0.0.4",
    "p-filter": "^1.0.0",
    "p-limit": "^2.2.0",
    "semver": "^5.7.0",
    "slay": "^1.0.0",
    "slay-config": "^2.0.1",
    "slay-log": "^2.1.0",
    "warehouse-models": "^5.9.1",
    "warehouse.ai-api-client": "~2.0.1",
    "winston": "~2.2.0"
  },
  "devDependencies": {
    "assume": "^2.2.0",
    "assume-sinon": "^1.0.1",
    "clone": "^2.1.2",
    "eslint": "^5.16.0",
    "eslint-config-godaddy": "^3.0.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-mocha": "^5.3.0",
    "mermaid.cli": "^0.5.1",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nyc": "^13.3.0",
    "request": "^2.78.0",
    "sinon": "^7.3.2"
  }
